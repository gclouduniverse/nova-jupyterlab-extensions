{
  "name": "jupyterlab_gcpscheduler",
  "version": "0.3.1",
  "description": "GCP Notebooks Scheduler Extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/gclouduniverse/nova-jupyterlab-plugin",
  "bugs": {
    "url": "https://github.com/gclouduniverse/nova-jupyterlab-plugin/issues"
  },
  "license": "MIT",
  "author": "GCP AI Platform UI Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/gcpsettings.json",
    "style/*.css",
    "style/images/*.png"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/gclouduniverse/nova-jupyterlab-plugin.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "lint-warnings": "eslint \"src/**/*.{ts,tsx}\" --fix --quiet",
    "lint": "npm run lint-warnings -- --quiet",
    "start": "concurrently -k -p \"[{name}]\" -n \"Typescript,JupyterLab\" -c \"green.bold,yellow.bold\" \"npm run watch\" \"jupyter lab --watch\"",
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.2",
    "@jupyterlab/apputils": "^1.0.2",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/docregistry": "^1.0.2",
    "@jupyterlab/filebrowser": "^1.0.3",
    "@jupyterlab/mainmenu": "^1.0.2",
    "@jupyterlab/notebook": "^1.0.2",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@phosphor/disposable": "^1.2.0",
    "@phosphor/widgets": "^1.8.1",
    "csstips": "^1.1.0",
    "formik": "^1.5.8",
    "typestyle": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@jupyterlab/testutils": "^1.0.2",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/fs-extra": "^8.0.0",
    "@types/gapi": "0.0.39",
    "@types/gapi.client.cloudfunctions": "^1.0.1",
    "@types/gapi.client.ml": "^1.0.1",
    "@types/gapi.client.servicemanagement": "^1.0.1",
    "@types/gapi.client.storage": "^1.0.1",
    "@types/jest": "^24.0.16",
    "@types/react": "~16.4.18",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "concurrently": "^4.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "jest-junit": "^7.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.1",
    "simulate-event": "^1.4.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "react": "~16.4.2",
    "react-dom": "~16.4.2"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "discovery": {
      "server": {
        "base": {
          "name": "jupyterlab_gcpscheduler",
          "managers": [
            "pip"
          ]
        }
      }
    }
  }
}
