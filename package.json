{
  "name": "nova",
  "version": "0.3.1",
  "description": "Jupyter Notebook submission to GCP",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/gclouduniverse/nova-jupyterlab-plugin",
  "bugs": {
    "url": "https://github.com/gclouduniverse/nova-jupyterlab-plugin/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Abdalla Abdulmonem, Viacheslav Kovalevskyi",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/*.css",
    "style/images/*.png"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/gclouduniverse/nova-jupyterlab-plugin.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "start": "concurrently -k -p \"[{name}]\" -n \"Typescript,JupyterLab\" -c \"green.bold,yellow.bold\" \"npm run watch\" \"jupyter lab --watch\"",
    "test": "jest --coverage",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.2",
    "@jupyterlab/apputils": "^1.0.2",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/docregistry": "^1.0.2",
    "@jupyterlab/filebrowser": "^1.0.3",
    "@jupyterlab/mainmenu": "^1.0.2",
    "@jupyterlab/notebook": "^1.0.2",
    "@phosphor/disposable": "^1.2.0",
    "@phosphor/widgets": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@jupyterlab/testutils": "^1.0.2",
    "@types/fs-extra": "^8.0.0",
    "@types/gapi": "0.0.39",
    "@types/gapi.client.cloudfunctions": "^1.0.1",
    "@types/gapi.client.ml": "^1.0.1",
    "@types/gapi.client.servicemanagement": "^1.0.1",
    "@types/gapi.client.storage": "^1.0.1",
    "@types/jest": "^24.0.16",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
    "jest-junit": "^7.0.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.1",
    "simulate-event": "^1.4.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "typestyle": "^2.0.4"
  },
  "jupyterlab": {
    "extension": true
  }
}
