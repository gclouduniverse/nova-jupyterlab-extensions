{
  "compilerOptions": {
    "declaration": true,
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "strictNullChecks": false,
    "target": "es2015",
    "types": [
      "gapi",
      "gapi.client",
      "gapi.client.servicemanagement",
      "gapi.client.storage"
    ]
  },
  "include": ["src"]
}
